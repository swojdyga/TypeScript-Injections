{
    "compilerOptions": {
        "module": "es6",
        "target": "es6",
        "moduleResolution": "node",
        "declaration": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "strict": true,
        "strictBindCallApply": true,
        "strictPropertyInitialization": true,
        "declarationDir": "build/es6",
        "outDir": "build/es6",
        "lib": [
            "es2019"
        ]
    },
    "include": [
        "src/index.ts"
    ]
}