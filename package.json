{
  "name": "typescript-injections",
  "version": "1.0.2",
  "description": "TypeScript Injections is a dependency injection library, which move responsibility from producer to customer.",
  "main": "./build/commonjs/index.js",
  "jsnext:main": "./build/es6/index.js",
  "types": "./build/es6/index.d.ts",
  "scripts": {
    "test": "mocha -r esm -r ts-node/register src/**/*.spec.ts tests/**/*.spec.ts",
    "build": "npm run build-commonjs && npm run build-es6",
    "build-commonjs": "(rimraf -r build/commonjs/* && tsc --project tsconfig-commonjs.json) || rimraf -r build/commonjs/*",
    "build-es6": "(rimraf -r build/es6/* && tsc --project tsconfig-es6.json) || rimraf -r build/es6/*",
    "main": "npm run test && npm run build",
    "publish": "npm run main && npm publish"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^7.0.2",
    "@types/node": "^10.17.9",
    "@vue/test-utils": "^1.0.3",
    "chai": "^4.2.0",
    "esm": "^3.2.25",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "mocha": "^7.2.0",
    "ts-loader": "^6.2.1",
    "ts-loader-decleration": "^0.10.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.8.3",
    "vue": "^2.6.11",
    "vue-property-decorator": "^8.4.2",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "typescript-class-types": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swojdyga/typescript-injections.git"
  },
  "keywords": [
    "typescript",
    "dependency-injection",
    "dependency-injection-container",
    "inversion-of-control",
    "dip",
    "ioc",
    "ioc-container",
    "javascript",
    "nodejs",
    "typescript-library",
    "javascript-library"
  ],
  "author": "Sebastian Wojdyga",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swojdyga/typescript-injections/issues"
  },
  "homepage": "https://github.com/swojdyga/typescript-injections#readme"
}
